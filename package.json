    "test:debug:e2e": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- -c jest-e2e.config.ts ",
    "test:unit": "npm test -- -c jest-unit.config.ts --coverage",
    "prepare": "husky install"
  },
